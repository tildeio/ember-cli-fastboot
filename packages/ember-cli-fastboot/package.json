{
  "name": "ember-cli-fastboot",
  "version": "3.2.0-beta.2",
  "description": "Server-side rendering for Ember.js apps",
  "keywords": [
    "ember-addon"
  ],
  "repository": "https://github.com/ember-fastboot/ember-cli-fastboot",
  "license": "MIT",
  "author": "Tom Dale & Yehuda Katz <tomhuda@tilde.io>",
  "directories": {
    "doc": "doc",
    "test": "tests"
  },
  "scripts": {
    "build": "ember build --environment=production",
    "lint": "npm-run-all --aggregate-output --continue-on-error --parallel 'lint:!(fix)'",
    "lint:fix": "npm-run-all --aggregate-output --continue-on-error --parallel lint:*:fix",
    "lint:hbs": "ember-template-lint .",
    "lint:hbs:fix": "ember-template-lint . --fix",
    "lint:js": "eslint . --cache",
    "release": "release-it",
    "lint:js:fix": "eslint . --fix",
    "start": "ember serve",
    "test": "npm-run-all lint test:*",
    "test:mocha": "mocha",
    "test:ember": "ember test",
    "test:precook": "node node_modules/ember-cli-addon-tests/scripts/precook-node-modules.js"
  },
  "dependencies": {
    "broccoli-concat": "^3.7.1",
    "broccoli-file-creator": "^2.1.1",
    "broccoli-funnel": "^2.0.1",
    "broccoli-merge-trees": "^3.0.1",
    "broccoli-plugin": "^1.3.1",
    "chalk": "^2.4.1",
    "ember-cli-babel": "^7.26.3",
    "ember-cli-htmlbars": "^5.7.1",
    "ember-cli-lodash-subset": "2.0.1",
    "ember-cli-preprocess-registry": "^3.1.2",
    "ember-cli-version-checker": "^3.0.0",
    "fastboot": "3.2.0-beta.2",
    "fastboot-express-middleware": "3.2.0-beta.2",
    "fastboot-transform": "^0.1.3",
    "fs-extra": "^7.0.0",
    "json-stable-stringify": "^1.0.1",
    "md5-hex": "^2.0.0",
    "recast": "^0.19.1",
    "silent-error": "^1.1.0"
  },
  "devDependencies": {
    "@ember/optional-features": "^2.0.0",
    "@ember/test-helpers": "^2.2.5",
    "@embroider/test-setup": "^0.37.0",
    "@glimmer/component": "^1.0.4",
    "@glimmer/tracking": "^1.0.4",
    "babel-eslint": "^10.1.0",
    "body-parser": "^1.18.3",
    "broccoli-asset-rev": "^3.0.0",
    "broccoli-test-helper": "^1.5.0",
    "co": "4.6.0",
    "chai": "^4.1.2",
    "chai-fs": "^2.0.0",
    "chai-string": "^1.4.0",
    "ember-auto-import": "^1.11.2",
    "ember-cli": "~3.26.1",
    "ember-cli-addon-tests": "^0.11.1",
    "ember-cli-dependency-checker": "^3.2.0",
    "ember-cli-inject-live-reload": "^2.0.2",
    "ember-cli-sri": "^2.1.1",
    "ember-cli-terser": "^4.0.1",
    "ember-disable-prototype-extensions": "^1.1.3",
    "ember-export-application-global": "^2.0.1",
    "ember-load-initializers": "^2.1.2",
    "ember-maybe-import-regenerator-for-testing": "^1.0.0",
    "ember-page-title": "^6.2.1",
    "ember-qunit": "^5.1.4",
    "ember-resolver": "^8.0.2",
    "ember-sinon": "^2.2.0",
    "ember-source": "~3.26.1",
    "ember-template-lint": "^3.2.0",
    "eslint": "^7.23.0",
    "eslint-config-prettier": "^8.1.0",
    "eslint-plugin-ember": "^10.3.0",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^3.3.1",
    "glob": "^7.1.3",
    "loader.js": "^4.7.0",
    "mocha": "^5.2.0",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.2.1",
    "qunit": "^2.14.1",
    "qunit-dom": "^1.6.0",
    "release-it": "^12.0.1",
    "release-it-lerna-changelog": "^1.0.2",
    "request": "^2.88.0",
    "rsvp": "^4.8.3"
  },
  "engines": {
    "node": "12.* || 14.* || >=16"
  },
  "publishConfig": {
    "registry": "https://registry.npmjs.org/"
  },
  "ember": {
    "edition": "classic"
  },
  "ember-addon": {
    "configPath": "tests/dummy/config",
    "before": [
      "broccoli-serve-files"
    ]
  },
  "volta": {
    "extends": "../../package.json"
  }
}
